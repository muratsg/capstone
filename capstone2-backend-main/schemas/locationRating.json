{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/location_rating.json",
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "minLength": 1,
        "maxLength": 15
      },
      "username": {
        "type": "string",
        "minLength": 3,
        "maxLength": 30
      },
      "rating": {
        "type": "integer",
        "minimum": 1,
        "maximum": 5
      }
    },
    "additionalProperties": false,
    "required": [
      "id",
      "username",
      "rating"
    ]
  }
  