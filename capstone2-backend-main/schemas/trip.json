{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/trip.json",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "distance": {
        "type": "number",
        "minimum": 0
      }
    },
    "additionalProperties": false,
    "required": [
      "name",
      "distance"
    ]
  }
  