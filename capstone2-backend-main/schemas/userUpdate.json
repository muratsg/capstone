{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/update.json",
    "type": "object",
    "properties": {
      "password": {
        "type": "string",
        "minLength": 5,
        "maxLength": 20
      },
      "zipCode": {
        "type": "string",
        "minLength": 1,
        "maxLength": 12
      },
      "country": {
        "type": "string",
        "minLength": 2,
        "maxLength": 2
      },
      "email": {
        "type": "string",
        "minLength": 6,
        "maxLength": 60,
        "format": "email"
      },
      "units": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1
      }
    },
    "additionalProperties": false,
    "required": []
  }